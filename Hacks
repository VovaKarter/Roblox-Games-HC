	local Teleport4 = main:Toggle("Kill Elite Gladiators", {flag="TpEliteGladiator"})
	local Teleport5 = main:Toggle("Kill Darkness Demons", {flag="TPDarknessDemonds"})
	local Teleport6 = main:Toggle("Kill Void Creatures", {flag="TpVoidCreatures"})
	local Teleport7 = main:Toggle("Kill God Creatures", {flag="TPGodCreatures"})
	local Teleport8 = main:Toggle("Kill God Lvl1", {flag="TpGodLvl1"})
	local Teleport9 = main:Toggle("Kill Other Gods", {flag="TpOtherGods"})
	spawn(function()
		while wait() do
			if main.flags.TPOn then
				local ooooooof = TextBox.Text
				local you = game.Players.LocalPlayer.Character
				local other = game.Workspace:FindFirstChild(ooooooof)
				if game.Workspace:FindFirstChild(ooooooof) and you:FindFirstChild("HumanoidRootPart") and other:FindFirstChild("HumanoidRootPart") then
					you:MoveTo(other.HumanoidRootPart.Position + other.HumanoidRootPart.CFrame.LookVector*3)
				end
			end
			if main.flags.TpNoobs then
				local you = game.Players.LocalPlayer.Character
				for i,v in pairs(game.Workspace.Npcs.Noobs:GetChildren()) do
					for j,n in pairs(v:GetChildren()) do
						if n:IsA("Part") then
							n.CanCollide = false
						end
					end
					v:MoveTo(you.HumanoidRootPart.Position + you.HumanoidRootPart.CFrame.LookVector*3)
				end
			end
			if main.flags.TpGlads then
				local you = game.Players.LocalPlayer.Character
				for i,v in pairs(game.Workspace.Npcs.Gladiators:GetChildren()) do
					for j,n in pairs(v:GetChildren()) do
						if n:IsA("Part") then
							n.CanCollide = false
						end
					end
					v:MoveTo(you.HumanoidRootPart.Position + you.HumanoidRootPart.CFrame.LookVector*3)
				end
			end
			if main.flags.TpEliteGladiator then
				local you = game.Players.LocalPlayer.Character
				for i,v in pairs(game.Workspace.Npcs["Elite Gladiator"]:GetChildren()) do
					for j,n in pairs(v:GetChildren()) do
						if n:IsA("Part") then
							n.CanCollide = false
						end
					end
					v:MoveTo(you.HumanoidRootPart.Position + you.HumanoidRootPart.CFrame.LookVector*3)
				end
			end
			if main.flags.TpDarknessDemonds then
				local you = game.Players.LocalPlayer.Character
				for i,v in pairs(game.Workspace.Npcs["Darkness Demonds"]:GetChildren()) do
					for j,n in pairs(v:GetChildren()) do
						if n:IsA("Part") then
							n.CanCollide = false
						end
					end
					v:MoveTo(you.HumanoidRootPart.Position + you.HumanoidRootPart.CFrame.LookVector*3)
				end
			end
			if main.flags.TpVoidCreatures then
				local you = game.Players.LocalPlayer.Character
				for i,v in pairs(game.Workspace.Npcs["Void Creatures"]:GetChildren()) do
					for j,n in pairs(v:GetChildren()) do
						if n:IsA("Part") then
							n.CanCollide = false
						end
					end
					v:MoveTo(you.HumanoidRootPart.Position + you.HumanoidRootPart.CFrame.LookVector*3)
				end
			end
			if main.flags.TPGodCreatures then
				local you = game.Players.LocalPlayer.Character
				for i,v in pairs(game.Workspace.Npcs["God Creatures"]:GetChildren()) do
					for j,n in pairs(v:GetChildren()) do
						if n:IsA("Part") then
							n.CanCollide = false
						end
					end
					v:MoveTo(you.HumanoidRootPart.Position + you.HumanoidRootPart.CFrame.LookVector*3)
				end
			end
			if main.flags.TpGodLvl1 then
				local you = game.Players.LocalPlayer.Character
				for i,v in pairs(game.Workspace.Npcs["Infinity Gods"].lvl1:GetChildren()) do
					for j,n in pairs(v:GetChildren()) do
						if n:IsA("Part") then
							n.CanCollide = false
						end
					end
					v:MoveTo(you.HumanoidRootPart.Position + you.HumanoidRootPart.CFrame.LookVector*3)
				end
			end
			if main.flags.TpOtherGods then
				local you = game.Players.LocalPlayer.Character
				for i,v in pairs(game.Workspace.Npcs["Infinity Gods"]:GetChildren()) do
					if not v:IsA("Model") then continue end
					for j,n in pairs(v:GetChildren()) do
						if n:IsA("Part") then
							n.CanCollide = false
						end
					end
					v:MoveTo(you.HumanoidRootPart.Position + you.HumanoidRootPart.CFrame.LookVector*3)
				end		
			end
		end
	end) 
