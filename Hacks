	local GameName = main:Section("BS:ED")

	local names = {
		"5 Gold",
		"50 Metal",
		"250 Press",
		"100 Microparticles",
		"12 Star",
		"12 Gold",
		"25 Ruby",
		"1 Gold"
	}

	function GetButtons()
		for i,v in pairs(workspace.Stardustry.Buttons:GetChildren()) do
			v.Name = v.InfoGUI.Give.Text
			if table.find(names, v.Name) then
				v.ButtonPart.CanCollide = false
				v.ButtonPart.Position = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
			end
		end	
	end

	local Get = main:Button("Get Sturdustry Buttons", function()
		GetButtons()
	end)
