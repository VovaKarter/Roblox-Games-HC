elseif game.PlaceId == 6456351776 then
	local ScreenGui = Instance.new("ScreenGui")
	local TextBox = Instance.new("TextBox")
	local CurrentPlayer = game.Players.LocalPlayer

	function GetTag(Char)
		if not Char then return end
		Char:WaitForChild("Head")
		Char.Head:WaitForChild("NameTag", 1):Destroy()
		local Attachment = Instance.new("Attachment", Char.Head)
		Attachment.Position += Vector3.new(0,2.2,0)
		Attachment.Name = "GUIAttachment"
		local Gui = Instance.new("BillboardGui", Attachment)
		Gui.Size = UDim2.new(4, 0, 2, 0)
		local Text = Instance.new("TextLabel", Gui)
		Text.Size = UDim2.new(1,0,1,0)
		Text.TextScaled = true
		Text.TextWrapped = true
		Text.Text = Char.Name
		Text.TextColor3 = Color3.fromRGB(255,255,255)
		Text.BackgroundTransparency = 1
	end

	ScreenGui.Parent = game.CoreGui

	TextBox.Parent = ScreenGui
	TextBox.BackgroundColor3 = Color3.new(0.333333, 0.333333, 0.333333)
	TextBox.Position = UDim2.new(0, 1760, 1, -50)
	TextBox.Size = UDim2.new(0, 150, 0, 25)
	TextBox.Font = Enum.Font.SourceSans
	TextBox.Text = "Insert Player Name"
	TextBox.TextColor3 = Color3.new(1, 1, 1)
	TextBox.TextScaled = true
	TextBox.TextSize = 14
	TextBox.TextWrapped = true


	local Showel = main:Section("Kaiju Paradise")

	local Teleport = main:Toggle("TPplayer", {flag="TPOn"})
	local FarmKills = main:Toggle("FarmKills (Other)", {flag="Farm"})
	local Rename = main:Toggle("RenamePlayers", {flag="Rename"})

	spawn(function()
		while game:GetService("RunService").RenderStepped:wait() do
			if main.flags.TPOn then
				local ooooooof = TextBox.Text
				local you = game.Players.LocalPlayer.Character
				local other = game.Workspace:FindFirstChild(ooooooof)
				if game.Workspace:FindFirstChild(ooooooof) and you:FindFirstChild("HumanoidRootPart") and other:FindFirstChild("HumanoidRootPart") then
					if CurrentPlayer.PlayerGui.MainGui.InventoryGui:FindFirstChild("1") then
						if CurrentPlayer.PlayerGui.MainGui.InventoryGui:FindFirstChild("1").Tool.Value == "Attack" then
							if main.flags.Farm then game.Players.LocalPlayer:LoadCharacter() end
							you:PivotTo(other.HumanoidRootPart.CFrame * CFrame.new(0,0,1))
						else
							if main.flags.Farm then
								you:PivotTo(other.HumanoidRootPart.CFrame * CFrame.new(0,0,-1))
							else
								you:PivotTo(other.HumanoidRootPart.CFrame * CFrame.new(0,0,2))
							end
						end
					else
						if main.flags.Farm then
							you:PivotTo(other.HumanoidRootPart.CFrame * CFrame.new(0,0,-1))
						else
							you:PivotTo(other.HumanoidRootPart.CFrame * CFrame.new(0,0,2))
						end
					end
				end
			end
		end
	end)

	game.Players.PlayerAdded:Connect(function(plr)
		repeat wait(.1) until plr.Character
		if main.flags.Rename then
			GetTag(plr.Character)
		end
		plr.CharacterAdded:Connect(function(Char)
			if main.flags.Rename then
				GetTag(Char)
			end
		end)
	end)

	for i,v in pairs(game.Players:GetChildren()) do
		if main.flags.Rename then
			GetTag(v.Character)
		end
		v.CharacterAdded:Connect(function(Char)
			if main.flags.Rename then
				GetTag(Char)
			end
		end)
	end
