	local AutoStage = main:Toggle("AutoStage", {flag="AutoStageON"})
	spawn(function()
		while game:GetService("RunService").RenderStepped:wait() do
			if main.flags.AutoStageON and dbs == false then
				local you = game.Players.LocalPlayer.Character
				if f <= 92 then
					you.HumanoidRootPart.CFrame = game.Workspace.Checkpoints["Checkpoint "..f].CFrame
					f += 1
					dbs = true
					wait(0.1)
					dbs = false
				end
			end
		end
	end)

